<div class="container">
    <div class="row">
      <div class="col">
        <div class="text-center">
          <h1>新規投稿画面</h1><%= form_with model: @movie, url: public_movies_path, local: true do |form| %>
          <h4>画像</h4><%= form.file_field :image, accept: "image/*" %>
          <h4>作品名</h4><%= form.text_field :title %>
          <h4>詳細</h4><%= form.text_area :body %><br>
          <%= form.submit %> <%end %>
        </div>
      </div>
    </div>
  </div>
</body>


